<h1><%=@artist[:name]%></h1>
<div class="albums">
  <form action="/artists/<%=@artist[:id]%>" method="post">
    <input name="_method" type="hidden" value="patch">
    <label for="artists_name">Change Artist Name</label>
    <input name="artists_name" type="text" class="form_control">
    <button type="submit" class="btn">Update!</button>
  </form>
  <% @albums.each do |album| %>
    <div class="album">
      <%= album[:name] %>
    </div>
  <% end %>
</div>
